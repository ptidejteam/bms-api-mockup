						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "<?xml version=\"1.0\" encoding=\"utf-8\" ?>\n<Struct xmlns=\"http://bacnet.org/csml/1.2\">\n    <List name=\"values\">\n        <Real name=\"1\" value=\"33\" via=\"/.bacnet/MainSite/5600/analog-value,1/present-value\"/>\n        <Real name=\"2\" value=\"44\" via=\"/.bacnet/MainSite/5600/analog-value,2/present-value\"/>\n        <Real name=\"3\" value=\"66\" via=\"/.bacnet/MainSite/5600/analog-value,3/present-value\"/>\n        <Real name=\"4\" value=\"88\" via=\"/.bacnet/MainSite/5600/analog-value,4/present-value\"/>\n    </List>\n</Struct>",
							"options": {
								"raw": {
									"language": "xml"
								}
							}
						},
						"url": {
							"raw": "http://127.0.0.1:5000/enteliweb/api/.multi",
